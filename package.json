{
  "name": "@stdjs/cache",
  "version": "0.1.1",
  "description": "Cache Adapter with Async/Promise for Javascript(& Typescript).",
  "keywords": [
    "cache-adapter",
    "cache",
    "adapter",
    "redis",
    "memcached",
    "elasticache",
    "async",
    "promise",
    "typescript"
  ],
  "author": "Cris <corgidisco@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/corgidisco/stdjs-cache.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepack": "npm run build",
    "build": "rimraf ./dist && tsc -p .",
    "lint": "tslint -c ./tslint.json -p tsconfig.json 'src/**/*.ts' --format stylish",
    "jest": "jest test",
    "test": "npm run build && npm run lint && npm run jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "[^/]*\\.(test|spec)\\.(j|t)sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/memcached": "^2.2.5",
    "@types/node": "^10.7.0",
    "@types/redis": "^2.8.6",
    "jest": "^23.5.0",
    "memcached": "^2.2.2",
    "redis": "^2.8.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^3.0.1"
  }
}
