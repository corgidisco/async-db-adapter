{
  "name": "relater",
  "version": "0.0.1",
  "description": "Decorator-based transformer of plain object to class constructors for Javascript(& Typescript).",
  "keywords": [
    "serialization",
    "serialize",
    "object-to-class",
    "datamapper",
    "data mapper",
    "typescript"
  ],
  "author": "Cris <corgidisco@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/corgidisco/relater.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepack": "npm run test && npm run build",
    "build": "rimraf ./lib && tsc --emitDeclarationOnly && babel src --out-dir lib --extensions \".ts\" --source-maps inline",
    "test": "npm run build && npm run test:lint && npm run test:unit",
    "test:lint": "eslint src/**/*",
    "test:unit": "jest test"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "[^/]*\\.(test|spec)\\.(j|t)sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^24.0.0",
    "@types/node": "^11.9.4",
    "@typescript-eslint/eslint-plugin": "^1.4.0",
    "eslint": "^5.14.1",
    "jest": "^24.1.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.0",
    "typescript": "^3.2.2"
  }
}
