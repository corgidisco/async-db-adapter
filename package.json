{
  "name": "@stdjs/database",
  "version": "1.3.1",
  "description": "Database Adapter with Async/Promise for Javascript(& Typescript).",
  "keywords": [
    "database-adapter",
    "db-adapter",
    "database",
    "db",
    "adapter",
    "async",
    "mysql",
    "mysql2",
    "pg",
    "postgre",
    "sqlite3",
    "sqlite",
    "aurora",
    "auroradb",
    "rds",
    "aws",
    "typescript"
  ],
  "author": "Cris <corgidisco@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/corgidisco/stdjs-database.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepack": "npm run test && npm run build",
    "build": "rimraf ./lib && tsc --emitDeclarationOnly && babel src --out-dir lib --extensions \".ts\" --source-maps inline",
    "test": "npm run build && npm run test:lint && npm run test:unit",
    "test:lint": "eslint src/**/*.ts",
    "test:unit": "jest test"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "[^/]*\\.test.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {
    "core-js": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^24.0.0",
    "@types/mysql": "^2.15.4",
    "@types/node": "^11.13.4",
    "@types/pg": "^7.4.10",
    "@types/sqlite3": "^3.1.3",
    "@typescript-eslint/eslint-plugin": "^1.4.0",
    "eslint": "^5.14.1",
    "jest": "^24.1.0",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "pg": "^7.4.3",
    "rimraf": "^2.6.2",
    "sqlite3": "^4.0.1",
    "ts-jest": "^24.0.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  }
}
