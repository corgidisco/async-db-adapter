{
  "name": "@stdjs/database",
  "version": "1.3.1",
  "description": "Database Adapter with Async/Promise for Javascript(& Typescript).",
  "keywords": [
    "database-adapter",
    "db-adapter",
    "database",
    "db",
    "adapter",
    "async",
    "mysql",
    "mysql2",
    "pg",
    "postgre",
    "sqlite3",
    "sqlite",
    "aurora",
    "auroradb",
    "rds",
    "aws",
    "typescript"
  ],
  "author": "Cris <corgidisco@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/corgidisco/stdjs-database.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepack": "npm run build",
    "build": "rimraf ./dist && tsc -p .",
    "lint": "tslint -c ./tslint.json -p tsconfig.json 'src/**/*.ts' --format stylish",
    "jest": "jest test",
    "test": "npm run build && npm run lint && npm run jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "[^/]*\\.(test|spec)\\.(j|t)sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/mysql": "^2.15.4",
    "@types/node": "^10.1.0",
    "@types/pg": "^7.4.10",
    "@types/sqlite3": "^3.1.3",
    "jest": "^23.6.0",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "pg": "^7.4.3",
    "rimraf": "^2.6.2",
    "sqlite3": "^4.0.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^3.1.6"
  }
}
