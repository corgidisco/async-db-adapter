{
  "name": "async-queue-adapter",
  "version": "0.0.1",
  "description": "Async Queue Adapter for Javascript(& Typescript).",
  "keywords": [
    "queue-adapter",
    "queue",
    "q",
    "adapter",
    "async",
    "typescript"
  ],
  "author": "Cris <corgidisco@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/corgidisco/async-db-adapter.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepack": "npm run build",
    "build": "rimraf ./dist && tsc -p .",
    "lint": "tslint -c ./tslint.json -p tsconfig.json 'src/**/*.ts' --format stylish",
    "jest": "jest test",
    "test": "npm run build && npm run lint && npm run jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "[^/]*\\.(test|spec)\\.(j|t)sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.0",
    "aws-sdk": "^2.292.0",
    "dotenv": "^6.0.0",
    "jest": "^23.5.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.3",
    "ts-node": "^3.3.0",
    "tslint": "^5.11.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^3.0.1"
  }
}
